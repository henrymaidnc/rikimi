{
    "GroupName": "k8s-cluster-sg",
    "Description": "Security group for Kubernetes cluster",
    "VpcId": "YOUR_VPC_ID",
    "IpPermissions": [
        {
            "IpProtocol": "tcp",
            "FromPort": 22,
            "ToPort": 22,
            "IpRanges": [
                {
                    "CidrIp": "0.0.0.0/0",
                    "Description": "SSH access"
                }
            ]
        },
        {
            "IpProtocol": "tcp",
            "FromPort": 6443,
            "ToPort": 6443,
            "IpRanges": [
                {
                    "CidrIp": "0.0.0.0/0",
                    "Description": "Kubernetes API server"
                }
            ]
        },
        {
            "IpProtocol": "tcp",
            "FromPort": 80,
            "ToPort": 80,
            "IpRanges": [
                {
                    "CidrIp": "0.0.0.0/0",
                    "Description": "HTTP"
                }
            ]
        },
        {
            "IpProtocol": "tcp",
            "FromPort": 443,
            "ToPort": 443,
            "IpRanges": [
                {
                    "CidrIp": "0.0.0.0/0",
                    "Description": "HTTPS"
                }
            ]
        }
    ]
} 